    "express": "^4.17.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/uuid": "^3.4.5",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
